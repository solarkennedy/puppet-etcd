addr               = "<%= @addr -%>"
bind_addr          = "<%= @bind_addr -%>"
ca_file            = "<%= @ca_file -%>"
cert_file          = "<%= @cert_file -%>"
<% if @cors and !@cors.empty? -%>
cors               = ["<%=  @cors.join('", "') -%>"]
<% end -%>
cpu_profile_file   = "<%= @cpu_profile_file -%>"
data_dir           = "<%= @data_dir -%>"
key_file           = "<%= @key_file -%>"
<% if @peers and !@peers.empty? -%>
peers              = ["<%=  @peers.join('", "') -%>"]
<% end -%>
<% if @peers_file -%>
peers_file         = "<%= @peers_file -%>"
<% end -%>
<% if @discovery -%>
discovery          = "<%= @discovery_endpoint -%><%= @discovery_token -%>"
<% end -%>
max_result_buffer  = <%= @max_result_buffer %>
max_retry_attempts = <%= @max_retry_attempts %>
name               = "<%= @node_name -%>"
snapshot           = <%= @snapshot %>
snapshot-count     = <%= @snapshot_count %>
verbose            = <%= @verbose %>
very_verbose       = <%= @very_verbose %>

[peer]
addr               = "<%= @peer_addr -%>"
bind_addr          = "<%= @peer_bind_addr -%>"
ca_file            = "<%= @peer_ca_file -%>"
cert_file          = "<%= @peer_cert_File -%>"
key_file           = "<%= @peer_key_file -%>"
election_timeout   = <%=  @peer_election_timeout %>
heartbeat_interval = <%=  @peer_heartbeat_interval %>
